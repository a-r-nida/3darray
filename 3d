include<stdio.h>
int main()
{
    int i,j,k;
int arr1[2][2][2];
for (i=0;i<=2;i++)
for (j=0;j<=2;j++)
for (k=0;k<=2;k++)
{
    scanf("%d%d%d\n",&arr1[i][j][k]);
}
printf("elements of array are\n");
for (i=0;i<=2;i++)
for (j=0;j<=2;j++)
for (k=0;k<=2;k++)
{ printf("%d%d%d\n",arr1[i][j][k]); }
getch();

}